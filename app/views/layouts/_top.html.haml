%nav.primary-bg.border-gray-200.fixed.w-full.z-20.top-0.start-0
  .flex.flex-wrap.items-center.justify-between.mx-auto.px-4.py-3
    %a.flex.items-center.space-x-3.rtl:space-x-reverse{:href => "#"}
      =image_tag 'chandra_herbal_logo.jpeg', class: "h-8"
      %span.self-center.text-2xl.font-semibold.whitespace-nowrap.text-white Chandra Harbarls
    #navbar-multi-level.hidden.w-full.md:block.md:w-auto
      %ul.flex.flex-col.font-medium.p-4.md:p-0.mt-4.border.border-gray-100.rounded-lg.bg-gray-50.md:space-x-8.rtl:space-x-reverse.md:flex-row.md:mt-0.md:border-0.primary-bg
        = active_link_to "Home", root_path, active: [['home'], []], wrap_tag: :li, class_active: 'public-nav-active', wrap_class: 'block py-2 px-3 text-white rounded hover:bg-blue-700'
        = active_link_to 'Products', products_path, active: [['products'], []], wrap_tag: :li, class_active: 'public-nav-active', wrap_class: 'block py-2 px-3 text-white rounded hover:bg-blue-700 hover:text-white'    
        = active_link_to carts_path, active: [['carts'], []], wrap_tag: :li, class_active: 'public-nav-active', wrap_class: 'block py-2 px-3 text-white rounded hover:bg-blue-700 hover:text-white', 'data-controller': 'root', 'data-root-target': 'link' do
          .flex
            %div.px-2 Cart
            %div.relative
              = heroicon 'shopping-cart', options: { class: "w-5 h-5" }
              %div.absolute.cart-count(data-cart-target="count") 0  
        - if !current_user.present?         
          %li 
            .block.py-2.px-3.text-gray-100.rounded.hover:bg-blue-700
              = link_to 'Sign in', new_user_session_path, data: { turbo_frame: 'global_modal_tag' }
          -# %li.block.py-2.px-3.text-gray-100.rounded.hover:bg-blue-700
            -# = button_to "Sign Out", destroy_admin_user_session_path, method: :delete
        - else
          %li 
            .flex.items-center
              .flex.items-center.ms-3
                %div
                  %button.flex.rounded-lg.focus:ring-4.focus:ring-blue-300.bg-white.w-auto.h-10.p-2{"aria-expanded" => "false", "data-dropdown-toggle" => "dropdown-user", type: "button"}
                    = heroicon 'user-circle',variant: :outline , options: {class: "w-6 h-6 mr-1"}
                    %div= current_user.first_name
                    = heroicon 'chevron-down',variant: :outline , options: {class: "w-4 h-4 ml-1 mt-1"}
                #dropdown-user.z-50.hidden.my-4.text-base.list-none.bg-white.divide-y.divide-gray-100.rounded.shadow
                  .px-4.py-3{role: "none"}
                    %p.text-sm.text-gray-900{role: "none"}
                      = current_user.full_name
                    %p.text-sm.font-medium.text-gray-900.truncate{role: "none"}
                      = current_user.email
                  %ul.py-1{role: "none"}
                    %li
                      .block.px-4.py-2.text-sm.text-gray-700.hover:bg-gray-100
                        = link_to user_path(current_user) do
                          = heroicon 'user-circle',variant: :outline , options: {class: "w-4 h-4 mr-1 inline-block "} 
                          %span Profile
                    %li
                      .block.px-4.py-2.text-sm.text-gray-700.hover:bg-gray-100
                        = link_to orders_path do
                          = heroicon 'cube',variant: :outline , options: {class: "w-4 h-4 mr-1 inline-block "} 
                          %span Orders
                    %li
                      .block.px-4.py-2.text-sm.text-gray-700.hover:bg-gray-100
                        = link_to orders_path do
                          = heroicon 'heart',variant: :outline , options: {class: "w-4 h-4 mr-1 inline-block "} 
                          %span Wishlist
                    %li
                      .block.px-4.py-2.text-sm.text-gray-700.hover:bg-gray-100
                        = button_to  destroy_user_session_path, method: :delete do 
                          = heroicon 'arrow-up-tray',variant: :outline , options: {class: "w-4 h-4 mr-1 inline-block "} 
                          %span Log Out





